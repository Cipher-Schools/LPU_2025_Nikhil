// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;

struct Node{
    int data;
    Node* next;
    
    Node(int value){
        data = value;
        next = NULL;
    }
};

void PrintLL( Node* head){
    Node* p = head;
    
    while(p!=NULL){
        cout<<p->data<<" ";
        p = p->next;
    }
}


