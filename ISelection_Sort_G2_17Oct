#include<iostream>
using namespace std;


int main(){
      
      int n = 5;
      int arr[n]={5,4,3,2,1};
      
      for(int i = 0;i<n;i++){
          
          int min_idx = i;
          
          for(int j = i;j<n;j++){
              if(arr[min_idx]>arr[j]){
                  min_idx = j;
              }
          }
          swap(arr[min_idx],arr[i]);
        
      }
      
      for(int i = 0;i<n;i++){
          cout<<arr[i]<<" ";
      }
      
    
}
