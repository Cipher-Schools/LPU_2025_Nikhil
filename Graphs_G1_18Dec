void DFS(int node ,  vector<vector<int>>&adjMatrix, vector<bool>&visited){
    visited[node]=true;
    cout<<node<<" ";
    
    // for(auto& x : adjMatrix[node]){
    //     for(auto& y : x){
    //          if(visited[y]==false) {
    //           DFS(y,adjMatrix,visited);
    //     }
       
    //     }
      
    // }
    // int v = adjMatrix[0].size();
    // for(int i = 0;i<=v;i++){
