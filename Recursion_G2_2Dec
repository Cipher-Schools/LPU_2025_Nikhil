// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;

int countOccuerence(int arr[],int idx,int target ,int n){
    if(idx>=n)return 0;
    
    int count = 0;
    if(arr[idx]==target)count++;
    
    return count + countOccuerence(arr,idx+1,target,n);
    
}

int sumofDigits(int n){
    if(n<=0)return 0;
    
    int rem = n%10;
    return rem + sumofDigits(n/10);
    
}

int firstOcc(int arr[],int idx,int target ,int n ){
    if(idx>=n)return -1;
    
    if(target==arr[idx])return idx;
    
    return firstOcc(arr,idx+1,target,n);
}

int lastOccurence(int arr[],int idx,int target,int n){
    if(idx>=n)return -1;
    
    int last =  lastOccurence(arr,idx+1,target,n);
    
    if(last!=-1)return last;
    
    return (target==arr[idx])?idx:-1;

}

int fibo(int n){
    if(n==0 || n==1)return n;
    
    return fibo(n-1) + fibo(n-2);
}

int factorial(int n){
    if(n==1)return 1;
    
    return n * factorial(n-1);
}

int pow(int a,int b){
    // if(b==0)return 1;
    // if(b==1)return a;    
    return a* pow(a,b-1);
}



