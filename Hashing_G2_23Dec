// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;

int main() {
    
    vector<int>vec = {1,1,1,2,2,1,3,4,5,6,7,3};
    unordered_map<int,int>mp;
    vector<int>freqArray(8);
    
    for(int i = 0;i<vec.size();i++){
        freqArray[vec[i]]++;
        mp[vec[i]]++;
    }
    
cout<<freqArray[1];
    
    return 0;
}
