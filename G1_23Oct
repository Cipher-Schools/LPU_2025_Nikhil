#include <iostream>
using namespace std;

int main() {
    //Insertion
    // int n = 5;
    // int arr[n]={3,2,1,4,2};
    // int pos = 2;
    // int value = 23;
    
    // int brr[n+1];
    // int i ;
    // for(i=0;i<pos;i++){
    //     brr[i]=arr[i];
    // }
    
 
    // brr[i] = value;
    //   // 3 2 23 1
    // //j = 4;
    //  //i= 3;
    // for(int j = pos+1 ;j<=n;j++){
    //     brr[j] =arr[i++];
    // }
    
    // for(int j = 0;j<=n;j++)cout<<brr[j]<<" ";
  
    int n = 7;
    int arr[n] = {12,8,9,2,3,4,5};
    int target  = 4;
    
    int l = 0 , r = n-1;
    
    while(l<=r){
        int m = (l+r)/2;
        if(arr[m]==target){
            cout<<m;
            break;
        }
        if(arr[m]<=arr[r]){
            
            if(target>=arr[m] && target<=arr[r]){
                l = m+1;
            }
            else{
                r = m-1;
            }
        }   
        else{
            if(target>=arr[l] && target<=arr[m]){
                r= m-1;
            }
            else l = m+1;
        }
        
    }
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
    return 0;
}
