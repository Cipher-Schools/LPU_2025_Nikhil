    // cout<<firstOcc;
    
    
    int n = 10;
    int arr[n]= {1,1,2,2,2,3,4,5,5,6};
    int l = 0,r = n-1;
    int lastOcc = -1;
    int target = 2;
    
    while(l<=r){
        int m = (l+r)/2;
        
        if(arr[m]==target){
            
            lastOcc = m;
            l = m+1;
        }
        else if(arr[m]>target){
            r= m-1;
        }
        else l = m+1;
    }
    cout<<lastOcc;
    
    
    
    
    

    return 0;
}
