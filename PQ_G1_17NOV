#include <iostream>
#include <vector>
#include <queue>
// #include <bits/stdc++.h>
using namespace std;

int main() {

    vector<int>vec = {1,3,5,7,8};

    priority_queue<int> pq ;  // Max Heap
    priority_queue<int,vector<int>,greater<int>> pq_min ;  // Min Heap


    //Operations
    // 1 . push
    // 2. pop 
    // 3  size 
    // 4  empty()
    // 5  top()

    for(auto& x  : vec){
        pq.push(x);
    }

    while(!pq.empty()){
        cout<<pq.top()<<" ";
        pq.pop();
    }

    //   while(pq.size()>0){
    //     cout<<pq.top()<<" ";
    //     pq.pop();
    // }


    // q - > Find the kth largest element in an array 

    int k = 3;

    while(pq.size()> 0 &&  k>1){
        pq.pop();
        k--;
    }
    cout<<pq.top();



}




