//   Map

    // map<int,int>mp;
    unordered_map<int,int>mp;
    
    vector<int>vec= {1,1,2,3,4,3,2,1,3,4,2,3,4,2,1,1,2,3,4};
    
    for(int i = 0;i<vec.size();i++){
        mp[vec[i]]++;
    }
    
    int max_ele = 0;
    int max_freq = 0;
    for(auto &x:mp){
        int key = x.first;
        int data = x.second;
        
        if(data>max_freq){
            max_freq= data;
            max_ele = key;
        }
        
    }
    
    cout<<max_ele<<" occured : "<<max_freq<<" times";
    
   
   

    return 0;
}

