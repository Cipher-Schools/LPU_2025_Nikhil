// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;

struct Node{
    int data;
    Node* next;
    
    Node(int value){
        data = value;
        next = NULL;
    }
};

void PrintLL( Node* head){
    Node* p = head;
    
    while(p!=NULL){
        cout<<p->data<<" ";
        p = p->next;
    }
}


int main() {
    
    Node* head = new Node(12);
    head->next = new Node(22);
    head->next->next = new Node(32);
    head->next->next->next = new Node(42);
    head->next->next->next->next = new Node(52);
    
    
    // Node* head2 = new Node(45);
    // head2->next = new Node(2);
    // head2->next->next = new Node(-32);
    // head2->next->next->next = head->next->next;
    //  //PrintLL(head);
     
    //  Node* p = head;
    //  Node* q = head2;
     
     
    //  while(p!=q){
    //   if(p==nullptr){
    //       p = head2;
    //   } else p = p->next;
       
    //   if(q==nullptr){
    //       q=head;
    //   }
    //      else q=q->next;
    //  }
     
    //  cout<<p->data;
     
     
    //  cout<<endl;
     
      PrintLL(head);
      cout<<endl;
      
      
      //Reverse the LL
     
     Node* p = head;
     Node* r = nullptr , *q = nullptr;
     
     while(p!=nullptr){
         r=q;
         q=p;
         p=p->next;
         
         q->next=r;
     }
     head = q;
     
     
   PrintLL(head);
     
     
     
     
     


    return 0;
}
