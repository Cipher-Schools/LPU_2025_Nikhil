class student{
    public :
    string name ;
    
    student(string nameOfStudent){
        name = nameOfStudent;
        cout<<name<<endl;
    }
    
};

class college{
  
  public : 
    student* s;
    
    
    college(student* st){
        s= st;
        cout<<"College is here"<<endl;
    }
    
    void print(){
        cout<<"Hello i am : "<<s->name;
    }
    
};

