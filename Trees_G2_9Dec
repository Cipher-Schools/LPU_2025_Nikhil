void printRootToLeaf(Node* node,vector<int>&nums){
     if(!node)return ;
     nums.push_back(node->data);
     if(node->left==nullptr && node->right == nullptr){
        for(int i = 0;i<nums.size();i++)cout<<nums[i]<<" ";
        cout<<endl;
    }

    printRootToLeaf(node->left,nums);
    printRootToLeaf(node->right,nums);
      nums.pop_back();
}


int main() {
    
    Node* root = new Node(1);
    Node* leftNode = new Node(2);
    Node* rightNode = new Node(3);
    
    root->left = leftNode ;
    root->right = rightNode;
    
    Node* left1 = new Node(4);
    leftNode->left = left1;
    Node* right1 = new Node(5);
    leftNode->right = right1;
  
      int arr[100];
    //   for(int i =0;i<100;i++)arr[i]=-1;
   // printLeftViewByLevelOrder(root);
    //   printLeftViewByPreOrder(root,0,arr);
    
    vector<int>nums;
    cout<<"The Root to leaf paths are : "<<endl;
    printRootToLeaf(root,nums);
    

    return 0;
}
