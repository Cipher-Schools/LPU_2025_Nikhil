struct Node{
    int data;
    Node* next;
};

class Queue{
    Node* front;
    Node* rear;
    
    Queue(){
        front=nullptr;
        rear = nullptr;
    }
    
    void enqueue(int x){
        
        Node* newNode = new Node(x);
        
        if(rear==nullptr){
            rear= newNode;
            front = newNode;
            return ;
        }
        
        rear->next = newNode;
        rear=newNode;
        
    }
    
    void dequeue(){
        if(front==nullptr){
            cout<<"Queue is empty"<<endl;
        }
        else{
            Node * temp = front ;
            front=front->next;
            delete temp;
            
            if(front==nullptr){
                rear=nullptr;
            }
        }
    }
    
    bool isEmpty(){
        if(front == nullptr){
            return true;
        }
        else return false;
        
        // return rear==nullptr;
    }
    
    int getFront(){
        if(isEmpty()){
            return -1;
        }
        else{
            return front->data;
        }
    }
    
    void traversal(){
        Node* p = front;
        
        while(p!=nullptr){
            cout<<p->data<<" ";
            p = p->next;
        }
        
    }
    
}



int main() {
    
    Queue q ;
    q.enqueue(10);
    q.enqueue(20);
    q.enqueue(30);
    q.traversal();
    
    q.dequeue();
    
    q.traversal();
   
   
   
   

    return 0;
}
