    //Rotate The Array Right By K Steps 
    
    int n = 7;
    int k = 3;
    int arr[n] = {1,2,3,4,5,6,7};
    
    int brr[n];
    int j = 0;
    
    for(int i  = n-k; i<n ;i++){
        brr[j] = arr[i];
        j++;
    }
    
    for(int i = 0;i<(n-k) ;i++){
        brr[j]=arr[i];
        j++;
    }
    
  
    
    
    for(int k = 0;k<n;k++){
        cout<<brr[k]<<" ";
    }
    

}



    
